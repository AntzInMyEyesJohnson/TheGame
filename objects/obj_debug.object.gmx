        "#special: " + string(obj_player.special_power_up)+
        "#weapon: " + string(obj_player.attack_power_up)+
        "#time_slow: " + string(time_slow)+
        "#time_stop: " + string(time_stop));
    if (ds_list_empty(obj_player.combatList)){
        draw_text(320,225,"combatList is empty");
    } else {
        for(var i = 0;i &lt; ds_list_size(obj_player.combatList);i++){
            draw_text(320,225+(i*12),string(object_get_name(obj_player.combatList[| i])) + string(obj_player.combatList[| i]));
        }
    }
    //draw_text(320,175,"queue size: " + string(ds_queue_size(obj_samurai.swords_queue)));
    draw_set_color(c_black);
}
