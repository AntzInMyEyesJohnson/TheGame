    if(obj_player.player_champ == GUNSLINGER){        
        if (obj_player.state == scr_player_headshot){
            draw_set_blend_mode(bm_add);
            draw_set_alpha(image_alpha);
            draw_line_width_colour(start_x,start_y,x,y,2.5,make_colour_hsv(235,240,100),make_colour_hsv(235,240,200));
