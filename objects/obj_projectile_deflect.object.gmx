            <string>///move projectile
//set depth
depth = -y;

//screen freeze for when player projectile hits an enemy
if (ScreenFreezeCheck()){exit;}

if (instance_exists(creator)){
    if (spd == 0){ 
        if (creator.object_index == obj_ninja){
            damage = 1;
            spd = 4;
            rotate = true;
            sprite_index = spr_enemy_projectile_bullet;
            image_index = 2;
            image_speed = 0;
        }
        if (creator.object_index == obj_soldier){
            damage = 1;
            spd = 5;
            //knockback = 4;
            rotate = false;
            sprite_index = spr_enemy_projectile_bullet;
            image_index = 3;
            image_speed = 0;
        }
    }
        hspd = lengthdir_x(spd,accuracy);
        vspd = lengthdir_y(spd,accuracy);
        decel = 1;
} else {
    instance_destroy();
}

hitting_enemy = instance_place(x,y,obj_enemy_parent);

if (hitting_enemy) {
    hspd -= hspd/1.4;
    vspd -= vspd/1.4;
}

if (bounce &gt;= bounce_max) or (abs(hspd) &lt;= 3 and abs(vspd) &lt;= 3){
    instance_destroy();
}

ProjectileBounceCollisionWithWallGrid();


