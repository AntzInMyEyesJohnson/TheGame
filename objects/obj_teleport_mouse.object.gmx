//check for player special ability input
if (instance_exists(obj_bandit)){
    depth = -10001;
    if (obj_bandit.state == 'assassinate'){
        //image effects
        image_alpha = .6;
        image_blend = c_red;
        image_angle += 5;
        //assassinate location animation
        if (image_index == 4){
            image_index = 5;
        } else if (image_index == 5){
            image_index = 4;
        }
        //set position to teleport spot
        x = obj_bandit.teleport_xpos;
        y = obj_bandit.teleport_ypos;
    }else if (obj_bandit.state == 'teleport'){
        //image effects
        image_alpha = .6;
        image_blend = c_aqua;
        image_angle += 5;
        //teleport location animation
        if (image_index == 4){
            image_index = 5;
        } else if (image_index == 5){
            image_index = 4;
        }
        //set position to teleport spot
        x = obj_bandit.teleport_xpos;
        y = obj_bandit.teleport_ypos;
    } else {
        if (ObjectInPath(obj_bandit.x,obj_bandit.y,x,y,10,obj_enemy_parent)){
            image_alpha = 1;
            image_blend = c_red;
            image_angle += 10;
        } else {
            image_alpha = .8;
            image_blend = c_white;
            image_angle = 0;
        }
        image_index = 4;
        //set position to the mouse
        x = mouse_x;
        y = mouse_y;
    }
}
